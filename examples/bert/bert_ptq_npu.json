{
  "input_model": {
    "type": "PyTorchModel",
    "model_loader": "load_hf_model",
    "model_script": "model.py",
    "model_path": "google-bert/bert-large-uncased",
    "io_config": {
      "input_names": ["hidden_states", "attention_mask"],
      "input_shapes": [
        [1, 512, 1024],
        [1, 1, 512, 512]
      ],
      "input_types": ["float32", "float32"],
      "output_names": ["logits"],
      "output_shapes": [[1, 512, 1024]]
    }
  },
  "engine": {
    "host": "host_system",
    "target": "target_system",
    "cache_dir": "temp/cache",
    "clean_cache": true,
    "clean_evaluation_cache": true,
    "output_dir": "outputs/google/bert_large"
  },
  "systems": {
    "host_system": {
      "type": "LocalSystem",
      "accelerators": [
        { "device": "cpu", "execution_providers": ["CPUExecutionProvider"] }
      ]
    },
    "target_system": {
      "type": "LocalSystem",
      "accelerators": [
        { "device": "npu", "execution_providers": ["QNNExecutionProvider"] }
      ]
    }
  },
  "data_configs": [
    {
      "name": "glue_mrpc",
      "type": "HuggingfaceContainer",
      "user_script": "model.py",
      "load_dataset_config": {
        "data_name": "glue",
        "subset": "mrpc",
        "split": "validation"
      },
      "pre_process_data_config": {
        "type": "bert_pre_process",
        "input_cols": ["sentence1", "sentence2"],
        "model_name": "google-bert/bert-large-uncased",
        "trust_remote_code": "true"
      },
      "dataloader_config": { "batch_size": 1 }
    }
  ],
  "passes": {
    "conversion": {
      "device": "cpu",
      "type": "OnnxConversion",
      "target_opset": 17,
      "save_as_external_data": true,
      "all_tensors_to_one_file": true,
      "dynamic": true,
      "use_dynamo_exporter": false
    },
    "qnn_preprocess": {
      "type": "QNNPreprocess",
      "fuse_layernorm": true,
      "save_as_external_data": true,
      "all_tensors_to_one_file": true
    },
    "quantization": {
      "type": "OnnxStaticQuantization",
      "data_config": "glue_mrpc",
      "quant_preprocess": true,
      "activation_type": "QUInt16",
      "weight_type": "QUInt8"
    }
  }
}
